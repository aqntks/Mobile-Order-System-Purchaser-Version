<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
             "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="skhu.mapper.SalesStatusMapper">

	<select id="findOne" resultType="SalesStatus">
		SELECT * FROM SalesStatus WHERE id =
		#{id}
	</select>

	<update id="update">
		UPDATE SalesStatus SET
		onoff2 = #{onoff2}
		WHERE
		id = #{id}
	</update>

</mapper>
